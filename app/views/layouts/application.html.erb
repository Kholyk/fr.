<script src="/template/js/revolution/extensions/revolution.extension.slideanims.min.js"></script>
<script src="/template/js/revolution/extensions/revolution.extension.video.min.js"></script>
<!--custom functions and script-->
<script src="/template/js/functions.js"></script>
